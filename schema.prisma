generator client {
    provider        = "prisma-client-js"
    previewFeatures = ["postgresqlExtensions"]
}

datasource db {
    provider   = "postgresql"
    url        = env("POSTGRES_PRISMA_URL")
    directUrl  = env("POSTGRES_URL_NON_POOLING")
    extensions = [vector]
}

model PostView {
    id        String   @id @default(cuid())
    slug      String
    visitedAt DateTime @default(now())
}
